{
  "extends": ["vue"],
  "plugins": ["vue", "async-await"],
  "rules": {
    "async-await/space-after-async": 2,
    "async-await/space-after-await": 2,
    "require-await": 2,
    "quotes": [2, "backtick", "single"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false
  }
}